{
  "name": "nordnet-ui-kit",
  "version": "0.1.0",
  "description": "Nordnets React powered UI kit",
  "main": "dist/nordnet-ui-kit.js",
  "scripts": {
    "analyze": "ANALYZE=true npm run webpack:bundle-use-rem",
    "clean": "rimraf dist",
    "build": "NODE_ENV=production npm-run-all clean webpack:bundle-use-rem webpack:bundle-use-rem:umd webpack:bundle webpack:individual-use-rem webpack:individual",
    "webpack:bundle-use-rem": "webpack --env.type bundle --env.use-rem --progress --colors --config webpack.config.babel.js",
    "webpack:bundle-use-rem:umd": "webpack --env.type bundle --env.libraryTarget umd --env.use-rem --progress --colors --config webpack.config.babel.js",
    "webpack:bundle": "webpack --env.type bundle --progress --colors --config webpack.config.babel.js",
    "webpack:individual-use-rem": "webpack --env.type individual --env.use-rem --progress --colors --config webpack.config.babel.js",
    "webpack:individual": "webpack --env.type individual --progress --colors --config webpack.config.babel.js",
    "test": "mocha-webpack --opts test/mocha.webpack.opts",
    "tdd": "npm test -- --watch",
    "lint": "eslint --ext=.jsx --ext=.js src test",
    "prepush": "npm-run-all lint test",
    "prepublish": "npm-run-all lint test build",
    "postpublish": "npm run clean && git push --follow-tags --no-verify && npm run docs-publish",
    "start": "npm run docs-server",
    "docs-server": "styleguidist server --colors",
    "docs-build": "rimraf documentation/build && styleguidist build",
    "predocs-publish": "npm run docs-build",
    "docs-publish": "git-directory-deploy --directory ./documentation/dist --branch gh-pages"
  },
  "keywords": [
    "nordnet",
    "react",
    "ui",
    "kit"
  ],
  "author": "Gustaf Zetterlund <gustaf.zetterlund@nordnet.se>",
  "repository": "nordnet/nordnet-ui-kit",
  "bugs": {
    "url": "https://github.com/nordnet/nordnet-ui-kit/issues"
  },
  "homepage": "https://nordnet.github.io/nordnet-ui-kit",
  "license": "MIT",
  "peerDependencies": {
    "react": "15.x",
    "react-dom": "15.x"
  },
  "dependencies": {
    "array-equal": "^1.0.0",
    "bezier-easing": "^2.0.3",
    "classnames": "^2.2.3",
    "color": "^0.11.3",
    "d3-interpolate": "^1.0.0",
    "lodash": "^4.17.4",
    "react-addons-shallow-compare": "^15.1.0",
    "react-motion-ui-pack": "^0.10.2",
    "react-prop-types": "^0.3.0",
    "react-select": "^1.0.0-rc.1",
    "react-svg-sprite-icon": "^0.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "6.14.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "enzyme": "^2.7.1",
    "eslint": "^2.0.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-config-nordnet": "^1.1.1",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "git-directory-deploy": "^1.5.0",
    "jsdom": "^9.11.0",
    "lodash-webpack-plugin": "^0.11.2",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-docgen": "^2.11.0",
    "react-dom": "^15.4.2",
    "react-styleguidist": "5.0.0-beta.11",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.1",
    "sass-variable-loader": "^0.1.1",
    "sinon": "^1.17.7",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "svg-icon-template-loader": "0.0.6",
    "svgo-loader": "^1.1.2",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-node-externals": "^1.2.0"
  }
}
